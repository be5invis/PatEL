with-semantics {
	export : lambda [form env] : match form {
		("export" id) [atom id] : Assign ("." [top.use "exports"] (".quote" id)) [env.use id] env false
		("export" id value) [atom id] : Assign ("." [top.use "exports"] (".quote" id)) value env false
		otherwise otherwise
	}
	begin {
		define [greet] [console.log "greet"]
		export greet
		export anotherGreet greet
		# export lalala lululu lololo ---- will cause an error
	}
}